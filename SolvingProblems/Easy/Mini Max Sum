public static void miniMaxSum(List<int> arr)
    {
        long min = arr.Min();
        long max = arr.Max();
        long minResult = 0;
        long maxResult = 0;
        
        foreach(int i in arr){
            minResult +=i;
            maxResult +=i;
        }
        Console.WriteLine((maxResult-max) + " " + (minResult-min));
    }
